<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane style="-fx-alignment: center;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d22.teamW.wApp.controllers.EquipmentListController">
   <children>
      <TableView fx:id="eqTab" layoutX="58.0" layoutY="56.0" onMouseClicked="#setSelected" prefHeight="466.0" prefWidth="305.0">
        <columns>
          <TableColumn prefWidth="99.0" text="MedID"><cellValueFactory>
              <PropertyValueFactory property="MedID" />
          </cellValueFactory> </TableColumn>
          <TableColumn prefWidth="104.0" text="Location"><cellValueFactory>
              <PropertyValueFactory property="Floor" />
          </cellValueFactory> </TableColumn>
            <TableColumn prefWidth="101.0" text="Status"><cellValueFactory>
                <PropertyValueFactory property="Status" />
            </cellValueFactory> </TableColumn>
        </columns>
      </TableView>
      <Button fx:id="cleanButton" layoutX="397.0" layoutY="56.0" mnemonicParsing="false" onAction="#markClean" prefHeight="25.0" prefWidth="201.0" text="Mark Selected Clean" />
      <Button fx:id="inuseButton" layoutX="397.0" layoutY="94.0" mnemonicParsing="false" onAction="#markInUse" prefHeight="25.0" prefWidth="201.0" text="Mark Selected In-Use" />
      <Button fx:id="dirtyButton" layoutX="397.0" layoutY="131.0" mnemonicParsing="false" onAction="#markDirty" prefHeight="25.0" prefWidth="201.0" text="Mark Selected Dirty" />
   </children>
</AnchorPane>
